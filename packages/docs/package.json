{
  "name": "@oruga-ui/docs-next",
  "version": "0.9.3",
  "homepage": "https://oruga-ui.com",
  "description": "UI components for Vue.js and CSS framework agnostic",
  "author": "Walter Tommasi <tommsi20@gmail.com>",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vitepress dev",
    "build": "vitepress build",
    "docs:dev": "concurrently \"npm run docs:gen:watch\" \"npm run dev\"",
    "docs:build": "npm run build",
    "docs:gen": "tsx src/docgen.ts",
    "docs:gen:watch": "tsx src/docgen.ts --watch",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@highlightjs/vue-plugin": "2.1.2",
    "@oruga-ui/theme-bootstrap": "0.7.2",
    "@oruga-ui/theme-bulma": "0.5.3",
    "@oruga-ui/theme-oruga": "0.5.1",
    "highlight.js": "11.11.1",
    "markdown-it": "^14.1.0",
    "markdown-it-highlightjs": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "9.1.2",
    "eslint": "^9.20.1",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.5.1",
    "sass": "^1.85.0",
    "tsx": "^4.19.2",
    "typescript": "5.7.3",
    "vitepress": "^1.6.3",
    "vue": "^3.5.13",
    "vue-component-meta": "^2.2.2",
    "vue-docgen-cli": "4.79.0",
    "vue-tsc": "^2.2.2"
  }
}
